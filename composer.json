  "type": "library",
  "version": "1.1.4",
  "require": {
    "php": ">=7.2.0",
    "ext-intl": "*",
    "ext-xml": "*",
    "ext-json": "*",
    "chillerlan/php-filereader": "^2.0",
    "symfony/console": "^5.0",
    "nesbot/carbon": "^2.24"
  },
  "license": "MIT",
  "autoload": {
